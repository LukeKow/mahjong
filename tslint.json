{
  "extends": ["tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules":{
    "whitespace": {
      "severity": "error",
      "options": [true, "check-preblock"]
    }
  }
}
